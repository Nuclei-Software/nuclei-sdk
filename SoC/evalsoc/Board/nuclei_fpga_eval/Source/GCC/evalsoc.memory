/* This file will be generated by nuclei_gen tool */

/* *_MEMROY_SIZE must be numerical only such as 0x10000 or 65536 */

/* SpiFlash Memory Information */
FLASH_MEMORY_PRESENT = 1;
FLASH_MEMORY_BASE = 0x20000000;
FLASH_MEMORY_SIZE = 0x800000;

/* SRAM Memory Information */
SRAM_MEMORY_PRESENT = 1;
SRAM_MEMORY_BASE = 0xA0000000;
SRAM_MEMORY_SIZE = 0x20000;
/* SRAM_MEMORY_ROM_SIZE must be smaller than SRAM_MEMORY_SIZE */
SRAM_MEMORY_ROM_SIZE = 0x10000;

/* MTVT and MTVEC Information */
/*
 * MTVT_BASE and MTVEC_BASE must be set with an value
 * If it is not present, you need to remove section in the linker script
 * MTVT_BASE: isr_vector section
 * MTVEC_BASE: exception section
 */
MTVT_BASE  = 0xA0000000;
MTVEC_BASE = 0xA0000084;
